  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HennadiiKupr/Astrozori/issues"
  },
  "homepage": "https://github.com/HennadiiKupr/Astrozori#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.9.3",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "gulp-htmllint": "^0.0.19",
    "htmllint": "^0.8.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1",
    "parcel": "^2.9.3",
    "stylelint": "^15.10.1",
